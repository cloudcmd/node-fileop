{
    "plugins": [
        "@babel/plugin-proposal-private-methods",
        "@babel/plugin-proposal-class-properties",
        "dynamic-import-node"
    ],
    "env": {
        "test": {
            "plugins": [
                "dynamic-import-node"
            ]
        },
        "client": {
            "presets": [
                "@babel/preset-env"
            ],
            "plugins": [
                "dynamic-import-webpack"
            ]
        }
    }
}
